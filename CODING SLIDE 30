package selfprogramming;

public class Slide30 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1;
		 do {	
			   if ((i % 2)== 0)
				System.out.println("Baki 0 " +i);
			   else
				   System.out.println("Baki selain daripada 0 " +i); 
			   i++;
		    } while(i<5);


	}

}
